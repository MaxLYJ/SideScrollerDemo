parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["A mother needs to be careful now. Needs her rest. "]},{"code":401,"indent":0,"parameters":["Carrying a new hope... a new life. After everything..."]},{"code":401,"indent":0,"parameters":["who would have thought?"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["(She stops humming. "]},{"code":401,"indent":0,"parameters":["The silence that follows is profound.)"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["(Her voice drops to a conspiratorial whisper, as if "]},{"code":401,"indent":0,"parameters":["sharing a secret with the yarn.) A little brother for "]},{"code":401,"indent":0,"parameters":["you... or a sister. A new beginning. Yes... a blessing "]},{"code":401,"indent":0,"parameters":["to wash away the old shadows."]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\pop[7]Do you want to see more?"]},{"code":102,"indent":0,"parameters":[["Not now","Go on"],-1,0,2,0]},{"code":402,"indent":0,"parameters":[0,"Not now"]},{"code":0,"indent":1,"parameters":[]},{"code":402,"indent":0,"parameters":[1,"Go on"]},{"code":201,"indent":1,"parameters":[0,5,13,9,0,0]},{"code":0,"indent":1,"parameters":[]},{"code":404,"indent":0,"parameters":[]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":1,"moveRoute":{"list":[{"code":25,"indent":null},{"code":15,"parameters":[200],"indent":null},{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":1,"moveType":3,"priorityType":0,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":24,"y":9}
